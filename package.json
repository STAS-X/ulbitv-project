{
	"name": "production-project",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000",
		"start:vite": "vite --port 3000",
		"start:dev": "concurrently \"npm start\" \"npm run start:json:server\"",
		"start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:json:server\"",
		"build:prod": "webpack --env mode=production --env apiURL=https://backend-sigma-beryl.vercel.app",
		"build:dev": "webpack --env mode=development",
		"start:json:server": "node ./json-server/index.js",
		"prettier": "npx prettier --write \"**/*.{ts,tsx,json}\"",
		"lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"lint:scss": "npx stylelint \"src/**/*.scss\"",
		"lint:scss:fix": "npx stylelint \"src/**/*.scss\" --fix",
		"prettier-ts-format": "prettier --config .prettierrc \"**/*.{ts,tsx}\" --write",
		"prettier-css-format": "prettier --config .prettierrc \"**/*.{css,scss}\" --write",
		"lint:prets:fix": "npm run prettier-ts-format & npm run lint:ts:fix",
		"lint:precss:fix": "npm run prettier-css-format & npm run lint:scss:fix",
		"test:unit": "jest --config ./config/jest/jest.config.json",
		"test:e2e": "cross-env isTest=true cypress open",
		"test:ui": "npx loki test",
		"test:ui:ok": "npx loki approve",
		"test:update": "loki update --reactUri file:./storybook-static",
		"test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
		"testV7:ui:ci": "concurrently --kill-others --raw --success first \"npm run storybook\" \"npm run test:ui\" \"cross-env PORT=6006 deelay\"",
		"test:ui:report": "npm run test:ui:json && npm run test:ui:html",
		"test:ui:json": "node scripts/generate-visual-json-report.js",
		"test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
		"test:storycap:ui": "npx storycap --reactUri file:./storybook-static -o \"_storycap_srceenshots_\"",
		"screen:ui": "npx storycap http://localhost:6006 -o \"_storycap_srceenshots_\"",
		"storybook": "sb dev -p 6006 -c ./config/storybook",
		"storybook:build": "sb build -c ./config/storybook",
		"prepare": "husky install",
		"postinstall": "patch-package && node scripts/clearCache ./node_modules/.cache"
	},
	"lint-stage": {
		"**/*.{ts?}": [
			"prettier --list-different",
			"eslint"
		]
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
