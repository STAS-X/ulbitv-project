	"overrides": {
		"chrome-remote-interface": "^0.32.1"
	},
	"loki": {
		"diffingEngine": "looks-same",
		"chromeSelector": "#storybook-root > *",
		"chromeTolerance": 10,
		"chromeRetries": 5,
		"chromeLoadTimeout": 120000,
		"chromeFlags": "--headless --disable-gpu --hide-scrollbars --no-sandbox",
		"configurations": {
			"chrome.laptop": {
				"target": "chrome.app",
				"width": 1440,
				"height": 800,
				"deviceScaleFactor": 1,
				"mobile": false
			},
			"chrome.iphone7": {
				"target": "chrome.app",
				"preset": "iPhone 7",
				"mobile": true
			}
		}
	}
